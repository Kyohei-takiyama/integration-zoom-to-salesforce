# AWSリージョン
aws_region = "ap-northeast-1"

# アプリケーション名と環境
app_name = "zoom-salesforce-integration"
environment = "dev"

# Zoom API設定
# .envファイルの値をここに設定します
zoom_webhook_secret_token = "xxxxx"
zoom_account_id = "xxxxx"
zoom_client_id = "xxxxx"
zoom_client_secret = "xxxxx"

# Salesforce API設定
# .envファイルの値をここに設定します
salesforce_base_url = "xxxxx"
salesforce_client_id = "xxxxx"
salesforce_client_secret = "xxxxx"